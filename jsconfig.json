{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["assets/*"],
			"@components/*": ["src/components/*"],
			"@config/*": ["src/config/*"],
			"@icons/*": ["src/icons/*"],
			"@screens/*": ["src/screens/*"],
			"@services/*": ["src/services/*"],
			"@store/*": ["src/store/*"]
		}
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist", ".expo"]
}
